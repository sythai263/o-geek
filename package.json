{
  "name": "o-geek_backend",
  "version": "0.0.1",
  "description": "Awesome NestJS Boilerplate, Typescript, Postgres, TypeORM",
  "author": "minh.hc <minh.hc@geekup.vn>",
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "start": "TZ=UTC nest start",
    "start:dev": "TZ=UTC nest start --watch",
    "start:debug": "TZ=UTC nest start --debug --watch",
    "format": "prettier --write \"src/**/*.ts\"",
    "start:hmr": "node dist/main",
    "migration": "ts-node node_modules/typeorm/cli.js migration:run -f ormconfig",
    "migration:generate": "ts-node node_modules/typeorm/cli.js migration:generate -f ormconfig -d src/migrations -n",
    "seeding:generate": "ts-node node_modules/typeorm/cli.js migration:generate -f ormconfig -d src/seeding -n",
    "migration:revert": "ts-node node_modules/typeorm/cli.js migration:revert -f ormconfig",
    "webpack": "webpack --config webpack.config.js --progress",
    "migration:create": "ts-node node_modules/typeorm/cli.js migration:create -f ormconfig -d src/migrations -n",
    "schema:drop": "ts-node node_modules/typeorm/cli.js schema:drop -f ormconfig",
    "start:prod": "node dist/src/main.js",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint --fix . --ext .ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "docs:dev": "vuepress dev -p 7070",
    "docs:build": "DEPLOY_ENV=gh-pages vuepress build",
    "docs:deploy": "yarn docs:build && gh-pages -d .vuepress/dist"
  },
  "dependencies": {
    "@nestjs/cli": "^7.5.1",
    "@nestjs/common": "^7.0.8",
    "@nestjs/core": "^7.0.8",
    "@nestjs/event-emitter": "^1.1.0",
    "@nestjs/jwt": "^7.0.0",
    "@nestjs/microservices": "^7.0.8",
    "@nestjs/passport": "^7.1.6",
    "@nestjs/platform-express": "^7.0.8",
    "@nestjs/schedule": "^1.0.2",
    "@nestjs/swagger": "^4.8.2",
    "@nestjs/typeorm": "^7.0.0",
    "@sentry/minimal": "^5.24.2",
    "@sentry/node": "^5.9.0",
    "@types/cron": "^1.7.3",
    "@types/webpack": "^5.28.0",
    "aws-sdk": "~2.683.0",
    "axios": "^0.26.1",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "dotenv": "~8.1.0",
    "express": "~4.17.1",
    "express-rate-limit": "^5.0.0",
    "express-session": "^1.17.2",
    "file-type": "~14.5.0",
    "handlebars": "^4.7.6",
    "helmet": "~3.22.0",
    "ical": "^0.8.0",
    "ical-generator": "^3.0.0",
    "jsonwebtoken": "~8.5.1",
    "lodash": "^4.17.21",
    "mime-types": "~2.1.27",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.33",
    "morgan": "^1.10.0",
    "nanoid": "^3.1.10",
    "passport": "^0.4.1",
    "passport-headerapikey": "^1.2.2",
    "passport-jwt": "^4.0.0",
    "passport-oauth2": "^1.6.1",
    "passport-strategy": "^1.0.0",
    "pg": "^8.2.1",
    "reflect-metadata": "^0.1.13",
    "request-context": "~2.0.0",
    "rxjs": "^6.6.0",
    "shallow-equal-object": "^1.1.1",
    "source-map-support": "^0.5.19",
    "swagger-ui-express": "^4.1.4",
    "typeorm": "^0.2.38",
    "typeorm-transactional-cls-hooked": "^0.1.12",
    "typescript": "^3.9.3",
    "uuid": "^8.1.0"
  },
  "devDependencies": {
    "@nestjs/testing": "^7.0.8",
    "@types/bcrypt": "^3.0.0",
    "@types/compression": "^1.7.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.6",
    "@types/express-rate-limit": "^5.0.0",
    "@types/express-session": "^1.17.4",
    "@types/file-type": "^10.9.1",
    "@types/helmet": "^0.0.47",
    "@types/jest": "^25.2.3",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.181",
    "@types/mime-types": "^2.1.0",
    "@types/morgan": "^1.9.0",
    "@types/node": "^14.0.5",
    "@types/passport": "^1.0.7",
    "@types/passport-http": "^0.3.9",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-strategy": "^0.2.35",
    "@types/supertest": "^2.0.9",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^3.0.0",
    "@typescript-eslint/eslint-plugin-tslint": "^3.0.0",
    "@typescript-eslint/parser": "^3.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-import-helpers": "^1.0.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "gh-pages": "^2.1.1",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "lint-staged": "~9.4.2",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2",
    "ts-jest": "^24.1.0",
    "ts-loader": "^7.0.4",
    "ts-node": "^8.10.1",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-consistent-codestyle": "^1.16.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-plugin-prettier": "^2.3.0",
    "vuepress": "^1.5.0",
    "webpack": "^4.41.1",
    "webpack-cli": "^3.3.9",
    "webpack-node-externals": "^1.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "git add"
    ]
  }
}
